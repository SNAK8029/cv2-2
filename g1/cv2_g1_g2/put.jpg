<script>
    $(document).ready(function(){
        $(document).keydown(function(k){
            if (k.ctrlKey == true && k.keyCode == 13)
            {
                var sel = getSel();
                if (sel.text.length > 200)
                {
                    alert('Вы выделили слишком много текста');
                    return;
                }
                var text = sel.pre + '<b style="color:red">' + sel.text + '</b>' + sel.suf;
                if (!text) return;
                $('.error-form').show().find('.text').html(text);
            }
            if (k.keyCode == 27)
            {
                $('.error-form').hide();
            }
        })
        $('.error-form').find('form').submit(function(e){
            e.preventDefault();
            var text = $('.error-form').find('.text').html();
            var comment = $('.error-form').find('[type="text"]').val();
            $('.error-form').find('[type="text"]').prop('disabled', true);
            $('.error-form').find('[type="submit"]').prop('disabled', true);
            $.ajax({
                url: '/ajax/ajax.php',
                type: "POST",
                data: {
                    action: 'mistake',
                    text: text,
                    comment: comment,
                    url: window.location.href
                },
                dataType: "json",
                cache: false,
                success: function(result){
                    if (typeof result.status != 'undefined')
                    {
                        alert(result.status == 'OK' ? 'Ваше сообщение об ошибке успешно отправлено, спасибо!' : 'При отправке произошла техническая ошибка. Пожалуйста, попробуйте еще раз.');
                    }
                },
                complete: function(){
                    $('.error-form').find('[type="text"]').prop('disabled', false);
                    $('.error-form').find('[type="submit"]').prop('disabled', false);
                    $('.error-form').hide();
                }

            })
            return false;
        })
        $('.error-form').find('.close').click(function(e){
            $('.error-form').hide();
        })
    })

    function getSel() {
        var n = null, r = null, c = 40;
        if (window.getSelection) {
            r = window.getSelection()
        }
        else {
            if (window.document.getSelection) {
                r = window.document.getSelection()
            }
            else {
                r = window.document.selection
            }
        }
        ;
        if (r != null) {
            var i = '', n = null, a = '', u = -1;
            if (r.getRangeAt) {
                var t = r.getRangeAt(0);
                n = t.toString();
                var s = window.document.createRange();
                s.setStartBefore(t.startContainer.ownerDocument.body);
                s.setEnd(t.startContainer, t.startOffset);
                i = s.toString();
                var o = t.cloneRange();
                o.setStart(t.endContainer, t.endOffset);
                o.setEndAfter(t.endContainer.ownerDocument.body);
                a = o.toString()
            }
            else {
                if (r.createRange) {
                    var t = r.createRange();
                    n = t.text;
                    var s = r.createRange();
                    s.moveStart('character', -c);
                    s.moveEnd('character', -n.length);
                    i = s.text;
                    var o = r.createRange();
                    o.moveEnd('character', c);
                    o.moveStart('character', n.length);
                    a = o.text
                }
                else {
                    n = '' + r
                }
            }
            ;
            var l, d = (l = n.match(/^(\s*)/)) && l[0].length,
                p = (l = n.match(/(\s*)$/)) && l[0].length;
            i = i + n.substring(0, d);
            a = n.substring(n.length - p, n.length) + a;
            n = n.substring(d, n.length - p);
            if (n == '') {
                return !1
            }
            ;
            i = i.substr(i.length - 30);
            a = a.substr(0, 30);
            return {pre: i, text: n, suf: a, pos: u}
        }
        else {
            alert(e.lng.not_supported);
            return !1
        }
    }
</script>
 
